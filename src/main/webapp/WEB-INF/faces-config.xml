<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    
    <application>
        <action-listener>org.primefaces.application.DialogActionListener</action-listener>
        <navigation-handler>org.primefaces.application.DialogNavigationHandler</navigation-handler>
        <view-handler>org.primefaces.application.DialogViewHandler</view-handler>        
        <resource-bundle>
            <base-name>/Bundle</base-name>
            <var>bundle</var>
        </resource-bundle>
        
        <navigation-rule>
            <navigation-case>
                <from-outcome>inicio</from-outcome>
            <to-view-id>http://localhost:8080/gestionPersonas/faces/index.xhtml</to-view-id>
            </navigation-case>
        </navigation-rule>    
    </application>
    
    <managed-bean>
        <managed-bean-name>mbLogin</managed-bean-name>
        <managed-bean-class>ar.gob.ambiente.servicios.gestionpersonas.managedBeans.MbLogin</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>mbUsuario</managed-bean-name>
        <managed-bean-class>ar.gob.ambiente.servicios.gestionpersonas.managedBeans.MbUsuario</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>mbActividad</managed-bean-name>
        <managed-bean-class>ar.gob.ambiente.servicios.gestionpersonas.managedBeans.MbActividad</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>mbDomicilio</managed-bean-name>
        <managed-bean-class>ar.gob.ambiente.servicios.gestionpersonas.managedBeans.MbDomicilio</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>mbEspecialidad</managed-bean-name>
        <managed-bean-class>ar.gob.ambiente.servicios.gestionpersonas.managedBeans.MbEspecialidad</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>mbEstablecimiento</managed-bean-name>
        <managed-bean-class>ar.gob.ambiente.servicios.gestionpersonas.managedBeans.MbEstablecimiento</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>mbEstado</managed-bean-name>
        <managed-bean-class>ar.gob.ambiente.servicios.gestionpersonas.managedBeans.MbEstado</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
