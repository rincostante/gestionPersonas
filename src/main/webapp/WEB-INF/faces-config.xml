    </managed-bean>
    <managed-bean>
        <managed-bean-name>mbPerfil</managed-bean-name>
        <managed-bean-class>ar.gob.ambiente.servicios.gestionpersonas.managedBeans.MbPerfil</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>mbTipoEstablecimiento</managed-bean-name>
        <managed-bean-class>ar.gob.ambiente.servicios.gestionpersonas.managedBeans.MbTipoEstablecimiento</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
