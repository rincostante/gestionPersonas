        <managed-bean-name>selectItemsBean</managed-bean-name>
        <managed-bean-class>ar.gob.ambiente.servicios.gestionpersonas.mb.SelectItemsBean</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
